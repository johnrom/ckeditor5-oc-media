{
  "name": "@johnrom/ckeditor5-oc-media",
  "version": "0.0.1",
  "description": "CKEditor5 Plugin for OrchardCore.Media",
  "keywords": [
    "CKEditor5",
    "Orchard Core",
    "Images"
  ],
  "dependencies": {
    "@ckeditor/ckeditor5-core": "^18.0.0",
    "@ckeditor/ckeditor5-image": "^18.0.0",
    "@ckeditor/ckeditor5-ui": "^18.0.0"
  },
  "devDependencies": {
    "eslint": "^6.8.0",
    "eslint-config-ckeditor5": "^2.1.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.1.3"
  },
  "engines": {
    "node": ">=8.0.0",
    "npm": ">=5.7.1"
  },
  "author": "John Rom",
  "license": "GPL-2.0-or-later",
  "homepage": "https://github.com/johnrom/ckeditor5-oc-image#readme",
  "bugs": {
    "url": "https://github.com/johnrom/ckeditor5-oc-image/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/johnrom/ckeditor5-oc-image.git"
  },
  "files": [
    "lang",
    "src",
    "theme"
  ],
  "scripts": {
    "lint": "eslint --quiet '**/*.js'"
  },
  "lint-staged": {
    "**/*.js": [
      "eslint --quiet"
    ]
  },
  "eslintIgnore": [
    "src/lib/**",
    "packages/**"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
